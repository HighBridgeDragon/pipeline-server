{
    "options": {},
    "pipeline": {
        "name": "audio_detection",
        "version": "environment"
    },
    "request": {
        "source": {
            "type": "gst",
            "element": "multifilesrc",
            "postproc": "wavparse ignore-length=1",
            "properties": {
                "location": "/home/pipeline-server/tests/media/how_are_you_doing.wav",
                "loop": true
            }
        },
        "destination": {
            "type": "file",
            "path": "/dev/null",
            "format": "json-lines"
        }
    },
    "stability_duration": 10800,
    "relaunch_on_complete": false,
    "numerical_tolerance": 0.2
}
