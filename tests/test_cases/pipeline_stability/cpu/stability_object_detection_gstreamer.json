{
    "options": {},
    "pipeline": {
        "name": "object_detection",
        "version": "person_vehicle_bike"
    },
    "request": {
        "source": {
            "type": "gst",
            "element": "multifilesrc",
            "postproc": "decodebin ! videoconvert ! video/x-raw,format=NV12",
            "capsfilter": "image/png,framerate=(fraction)30/1",
            "properties": {
                "location": "/home/pipeline-server/tests/media/sample01.png",
                "loop": true
            }
        },
        "destination": {
            "type": "file",
            "path": "/dev/null",
            "format": "json-lines"
        }
    },
    "stability_duration": 10800,
    "relaunch_on_complete": false
}
