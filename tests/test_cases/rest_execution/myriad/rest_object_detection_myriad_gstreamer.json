{
    "path": "pipelines/object_detection/person_vehicle_bike",
    "start" : {
        "body": {
            "source": {
                "uri": "file:///home/pipeline-server/tests/media/person-bicycle-car-detection.mp4",
                "type": "uri"
            },
            "destination": {
                "type": "file",
                "path": "/tmp/rest_object_detection",
                "format": "json-lines"
            },
            "parameters": {
                "detection-device": "MYRIAD",
                "detection-model-instance-id":""
            }
        }
    },
    "check_running" : {
        "timeout": 5.0
    },
    "check_stopped" : {
        "timeout": 100
    }
}
